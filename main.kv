<MainScreen>:
    name: 'main'
    FloatLayout:
        size_hint: None, None
        Image:
            source: "Logo4k.png"
            x: (root.width * .5)-100
            y: root.height * .7
            size_hint_x: 2
            allow_stretch: True

        Button:
            id: get_started
            text: "Get Started"
            x: (root.width * .5)-150
            y: root.height * 0.3
            size: 300, 70
            size_hint: None, None
            color: 1, 1, 1, 1
            bold: True
            background_color: 3, 1, 1, 1
            on_press: root.getStarted()




